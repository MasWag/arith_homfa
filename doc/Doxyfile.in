DOXYFILE_ENCODING      = UTF-8
PROJECT_NAME           = "ArithHomFA Developer Manual"
PROJECT_NUMBER         = @PROJECT_VERSION@
OUTPUT_DIRECTORY       = @CMAKE_BINARY_DIR@/doc/developer_manual
CREATE_SUBDIRS         = YES
ALLOW_UNICODE_NAMES    = NO
OUTPUT_LANGUAGE        = English
BRIEF_MEMBER_DESC      = YES
REPEAT_BRIEF           = YES
EXTRACT_ALL            = YES
EXTRACT_PRIVATE        = NO
EXTRACT_STATIC         = NO
EXTRACT_LOCAL_CLASSES  = NO
EXTRACT_ANON_NSPACES   = NO
FULL_PATH_NAMES        = NO
STRIP_FROM_PATH        = @CMAKE_SOURCE_DIR@
STRIP_FROM_INC_PATH    = @CMAKE_SOURCE_DIR@/src
INLINE_SOURCES         = YES
SOURCE_BROWSER         = YES
REFERENCES_RELATION    = YES
REFERENCED_BY_RELATION = YES
GENERATE_TREEVIEW      = YES
WARN_IF_UNDOCUMENTED   = YES
WARN_IF_DOC_ERROR      = YES
QUIET                  = NO

INPUT                  = @CMAKE_SOURCE_DIR@/src @CMAKE_SOURCE_DIR@/doc/docs
FILE_PATTERNS          = *.c *.cc *.cpp *.cxx *.h *.hh *.hpp *.hxx *.ipp *.md
RECURSIVE              = YES
USE_MDFILE_AS_MAINPAGE = @CMAKE_SOURCE_DIR@/doc/docs/developer-notes.md
MARKDOWN_SUPPORT       = YES
EXTENSION_MAPPING      = hh=C++ hpp=C++ hxx=C++

GENERATE_HTML          = YES
HTML_OUTPUT            = html
GENERATE_LATEX         = NO
GENERATE_MAN           = NO
GENERATE_RTF           = NO
GENERATE_XML           = NO
